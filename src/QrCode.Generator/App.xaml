<Application x:Class="WIFI.QRCode.Builder.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:WIFI.QRCode.Builder.ViewModels"
             xmlns:v="clr-namespace:WIFI.QRCode.Builder.Views"
             Startup="Application_Startup"
             Exit="Application_Exit">
  <Application.Resources>
    <DataTemplate DataType="{x:Type TypeName=vm:EventViewModel}">
      <v:EventCodeControl />
    </DataTemplate>
    <DataTemplate DataType="{x:Type TypeName=vm:WifiViewModel}">
      <v:WifiCodeControl />
    </DataTemplate>
    <DataTemplate DataType="{x:Type TypeName=vm:GiroCodeViewModel}">
      <v:GiroCodeControl />
    </DataTemplate>
  </Application.Resources>
</Application>
